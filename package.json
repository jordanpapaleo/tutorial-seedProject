{
  "name": "seed-project",
  "version": "1.0.0",
  "description": "A web seed project using webpack, react, and es6",
  "main": "./app/index.js",
  "scripts": {
    "start": "webpack && serve ./dist",
    "start:dev": "webpack",
    "test": "standard ./**/*.js, ./**/*.jsx, *.js | standard-reporter --stylish",
    "build-dist": "rm -rf ./dist && webpack --config webpack.config.js --bail -p",
    "dist": "npm test && npm run build-dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "6.3.26",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "css-loader": "0.23.1",
    "debug": "2.2.0",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "1.7.0",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "standard": "5.4.1",
    "standard-reporter": "1.0.5",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.9"
  },
  "dependencies": {
    "bootstrap": "3.3.6",
    "gsap": "1.18.2",
    "react": "0.14.5",
    "react-dom": "0.14.5"
  }
